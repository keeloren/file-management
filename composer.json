{
    "name": "keeloren/file-management",
    "description": "A Package for file management",
    "keywords": [
        "keeloren",
        "file-media"
    ],
    "homepage": "https://github.com/keeloren/file-management",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "keeloren",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "aws/aws-sdk-php": "^3.158",
        "laravel/framework": "^8.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "league/fractal": "^0.19.2",
        "prettus/l5-repository": "^2.6",
        "spatie/laravel-medialibrary": "^8.0.0",
        "superbalist/laravel-google-cloud-storage": "^2.2",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^2.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.2",
        "laravel/laravel": "*",
        "facade/ignition": "^2.3.6",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "GGPHP\\FileMedia\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GGPHP\\FileMedia\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "GGPHP\\FileMedia\\Providers\\FileMediaServiceProvider"
            ]
        }
    }
}
