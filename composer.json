{
    "name": "keeloren/file-management",
    "description": "A Package for file management",
    "keywords": [
        "keeloren",
        "file-media"
    ],
    "homepage": "https://github.com/keeloren/file-management",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "keeloren",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "laravel/framework": "^8.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "league/fractal": "^0.19.2",
        "prettus/l5-repository": "^2.6",
        "spatie/laravel-medialibrary": "^8.0.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.2",
        "laravel/laravel": "*"
    },
    "autoload": {
        "psr-4": {
            "GGPHP\\FileMedia\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GGPHP\\FileMedia\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "GGPHP\\FileMedia\\FileMediaServiceProvider"
            ]
        }
    }
}
