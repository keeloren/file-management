{
    "name": "keeloren/file-management",
    "description": "A Package for file management",
    "keywords": [
        "keeloren",
        "file-management"
    ],
    "homepage": "https://github.com/keeloren/file-management",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "keeloren",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "GGPHP\\FileManagement\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GGPHP\\FileManagement\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "GGPHP\\FileManagement\\UsersServiceProvider"
            ]
        }
    }
}
